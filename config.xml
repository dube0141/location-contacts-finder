<?xml version='1.0' encoding='utf-8' ?>
<widget id="m.edumedia.dube0141" version="0.0.502" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>Location &amp; Contact Finder</name>
	<description>
		A sample Apache Cordova application that finds the geolocation and a random contact from the device
	</description>
	<author email="dev@cordova.apache.org" href="http://cordova.io">
		Gabriel Dub√©
	</author>
	<content src="index.html" />
	<access origin="*" />
	<feature name="Geolocation">
		<param name="android-package" value="org.apache.cordova.geolocation" />
	</feature>
	<feature name="Contacts">
		<param name="android-package" value="org.apache.cordova.contacts" />
	</feature>
</widget>